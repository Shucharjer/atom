{
    "name": "electron",
    "dependencies": [
        "stdexec",
        "glm",
        {
            "name": "imgui",
            "features": [
                "docking-experimental"
            ]
        },
        "vulkan",
        "vulkan-headers",
        "vulkan-hpp",
        "vulkan-loader",
        "vulkan-memory-allocator",
        "vulkan-memory-allocator-hpp",
        {
            "name": "sdl3",
            "features": [
                "vulkan"
            ],
            "platform": "windows"
        },
        {
            "name": "sdl2",
            "features": [
                "vulkan",
                "x11"
            ],
            "platform": "linux"
        }
    ]
}